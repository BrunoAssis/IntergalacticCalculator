#!/usr/bin/env ruby

require "bundler/setup"
require "IntergalacticCalculator"

begin
  require "IntergalacticCalculator/runner.rb"
rescue LoadError => e
  require 'rubygems'
  path = File.expand_path '../../lib', __FILE__
  $:.unshift(path) if File.directory?(path) && !$:.include?(path)
  require "IntergalacticCalculator/runner.rb"
end
